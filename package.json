{
  "name": "normalize-error",
  "version": "0.0.0",
  "description": "Normalize unknown error values to a standard format.",
  "author": "Michael L. Hobbs",
  "homepage": "https://github.com/MichaelLeeHobbs/normalize-error/#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelLeeHobbs/normalize-error.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelLeeHobbs/normalize-error/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "audit-level": "high"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "build": "tsup --tsconfig tsconfig.build.json",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "lint-fix": "eslint --fix src/**/*.ts",
    "prepack": "pnpm run build && pnpm run lint && pnpm run test",
    "version:major": "pnpm version major",
    "version:minor": "pnpm version minor",
    "version:patch": "pnpm version patch",
    "preversion": "pnpm prepack",
    "postpublish": "pnpm view normalize-error",
    "dry-run": "pnpm npm pack --dry-run"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "keywords": [
    "typescript",
    "error",
    "errors",
    "types",
    "javascript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
